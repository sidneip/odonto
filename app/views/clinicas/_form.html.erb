<%= simple_form_for @clinica, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :nome %>
  <%= f.input :email %>
  <%= f.input :password	 %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                clinicas_path, :class => 'btn' %>
  </div>
<% end %>
