<%- model_class = ConsultaProcedimento -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:clinica_id) %>:</strong></dt>
  <dd><%= @consulta_procedimento.clinica_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:procedimento_id) %>:</strong></dt>
  <dd><%= @consulta_procedimento.procedimento_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:consulta_id) %>:</strong></dt>
  <dd><%= @consulta_procedimento.consulta_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:valor) %>:</strong></dt>
  <dd><%= @consulta_procedimento.valor %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              consulta_procedimentos_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_consulta_procedimento_path(@consulta_procedimento), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              consulta_procedimento_path(@consulta_procedimento),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
