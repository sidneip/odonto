<%- model_class = Convenio -%>
<div class="page-header">
  <h1>Visualizar Convenio></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:codigo_convenio) %>:</strong></dt>
  <dd><%= @convenio.codigo_convenio %></dd>
  <dt><strong><%= model_class.human_attribute_name(:cnpj) %>:</strong></dt>
  <dd><%= @convenio.cnpj %></dd>
  <dt><strong><%= model_class.human_attribute_name(:razao_social) %>:</strong></dt>
  <dd><%= @convenio.razao_social %></dd>
  <dt><strong><%= model_class.human_attribute_name(:nome_fantasia) %>:</strong></dt>
  <dd><%= @convenio.nome_fantasia %></dd>
  <dt><strong><%= model_class.human_attribute_name(:logradouro) %>:</strong></dt>
  <dd><%= @convenio.logradouro %></dd>
  <dt><strong><%= model_class.human_attribute_name(:numero) %>:</strong></dt>
  <dd><%= @convenio.numero %></dd>
  <dt><strong><%= model_class.human_attribute_name(:bairro) %>:</strong></dt>
  <dd><%= @convenio.bairro %></dd>
  <dt><strong><%= model_class.human_attribute_name(:complemento) %>:</strong></dt>
  <dd><%= @convenio.complemento %></dd>
  <dt><strong><%= model_class.human_attribute_name(:cep) %>:</strong></dt>
  <dd><%= @convenio.cep %></dd>
  <dt><strong><%= model_class.human_attribute_name(:uf) %>:</strong></dt>
  <dd><%= @convenio.uf %></dd>
  <dt><strong><%= model_class.human_attribute_name(:cidade) %>:</strong></dt>
  <dd><%= @convenio.cidade %></dd>
  <dt><strong><%= model_class.human_attribute_name(:telefone) %>:</strong></dt>
  <dd><%= @convenio.telefone %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fax) %>:</strong></dt>
  <dd><%= @convenio.fax %></dd>
  <dt><strong><%= model_class.human_attribute_name(:email) %>:</strong></dt>
  <dd><%= @convenio.email %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              convenios_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_convenio_path(@convenio), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              convenio_path(@convenio),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
